
<%= form_for @developer, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>
  <div class="form">
    <%= email_field_tag :email, nil, placeholder: "Email" %>
  </div>
  <div class = "form">
    <%= password_field_tag :password, nil, placeholder: "Password" %>
  </div>
  <div class = "form">
    <%= submit_tag "Login" %>
  </div>
<% end %>
