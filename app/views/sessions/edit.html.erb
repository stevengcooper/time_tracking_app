<h1>Edit your Login Information </h1>
<%= form_for @developer, url: {action: "update"}, html: {class: "nifty_form"} do |f| %>
  <div class="form">
    <%= email_field_tag :email, nil, placeholder: "Email" %>
  </div>
  <div class = "form">
    <%= password_field_tag :password, nil, placeholder: "Password" %>
  </div>
  <div class = "form">
    <%= submit_tag "Update" %>
  </div>
<% end %>
<br>
<div class="links">
  <%= link_to "Home", developers_path %>
</div>
