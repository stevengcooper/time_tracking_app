<%= form_for @time_entry do |f| %>

  <div class="form">
    <%= f.label :duration %>
    <%= f.text_field :duration %>
  </div>
  <div class="form">
    <%= f.label :date %>
    <%=  f.date_select :date %>
    <% f.hidden_field :date %>
  </div>
  <div class="form">
    <%= f.label :developer_id %>
    <%= f.collection_select :developer_id, Developer.where(id: session[:user_id]), :id, :name %>
  </div>
  <div class="form">
    <%= f.label :project_id %>
    <%= f.collection_select :project_id, Project.all, :id, :name %>
  </div>
  <div class="submitactions">
    <%= f.submit "Submit"%>
  </div>
<% end %>
