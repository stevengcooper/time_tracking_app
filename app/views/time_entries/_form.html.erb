<%= form_for @time_entry, url: {action: "create"} do |f| %>

  <div class="form">
    <%= f.label :duration %>
    <%= f.text_field :duration %>
  </div>
  <div class="form">
    <%= f.label :developer_id %>
    <%= f.collection_select :developer_id, Developer.all, :id, :name %>
    <% f.hidden_field :developer_id %>
  </div>
  <div class="form">
    <%= f.label :project_id %>
    <%= f.collection_select :project_id, Project.all, :id, :name %>
    <% f.hidden_field :project_id %>
  </div>
  <div class="submitactions">
    <%= f.submit "Submit" %>
  </div>
<% end %>
