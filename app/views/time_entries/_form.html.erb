<main class="cd-content">
  <div class="cd-container">
    <div class="form">
      <%= form_for @time_entry do |f| %>
        <div>
          <%= f.label :duration %>
          <%= f.text_field :duration %>
        </div>
        <div >
          <%= f.label :date %>
          <%=  f.date_select :date %>
          <% f.hidden_field :date %>
        </div>
        <div >
          <%= f.label :developer_id %>
          <%= f.collection_select :developer_id, Developer.where(id: session[:user_id]), :id, :name %>
        </div>
        <div >
          <%= f.label :project_id %>
          <%= f.collection_select :project_id, Project.all, :id, :name %>
        </div>
        <div class="submitactions">
          <%= f.submit "Submit"%>
        </div>
      <% end %>
    </div>
  </div>
</main>
